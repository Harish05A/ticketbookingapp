
{
  "version": 2,
  "builds": [
    {
      "src": "index.tsx",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    },
    {
      "src": "backend/moviebooking/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxDuration": 10 }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/moviebooking/wsgi.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
